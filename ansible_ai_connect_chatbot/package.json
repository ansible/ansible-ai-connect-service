{
  "name": "ansible-ai-connect-chatbot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@patternfly/chatbot": "^6.4.1",
    "@patternfly/react-core": "^6.0.0",
    "@patternfly/react-icons": "^6.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^6.4.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "postbuild": "rsync -a --delete ./dist/static/chatbot/ ../ansible_ai_connect/main/static/chatbot && rsync -a ./dist/index.html ../ansible_ai_connect/main/templates/chatbot/",
    "serve": "vite preview",
    "test": "vitest run --browser.headless src/",
    "coverage": "vitest run --browser.headless  --coverage src/",
    "eslint": "eslint --max-warnings=0 src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.13.0",
    "@testing-library/react": "^16.2.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "buffer": "^6.0.3",
    "empty-module": "^0.0.2",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-patternfly-react": "^5.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^16.0.0",
    "playwright": "^1.53.1",
    "process": "^0.11.10",
    "react-router-dom": "^7.5.2",
    "sass-embedded": "^1.79.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^0.0.4"
  }
}
