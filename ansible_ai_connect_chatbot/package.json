{
  "name": "ansible-ai-connect-chatbot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@patternfly/chatbot": "^2.2.0-prerelease.8",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0",
    "vite": "^5.4.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "postbuild": "rsync -a --delete ./dist/static/chatbot/ ../ansible_ai_connect/main/static/chatbot && rsync -a ./dist/index.html ../ansible_ai_connect/main/templates/chatbot/",
    "serve": "vite preview",
    "test": "vitest run --browser.headless src/",
    "coverage": "vitest run --browser.headless  --coverage src/",
    "eslint": "eslint --max-warnings=0 src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@vitest/browser": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.2",
    "buffer": "^6.0.3",
    "dompurify": "^3.2.3",
    "empty-module": "^0.0.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-patternfly-react": "^5.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "playwright": "^1.49.1",
    "process": "^0.11.10",
    "react-router-dom": "^6.26.2",
    "sass-embedded": "^1.79.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vitest": "^2.1.2",
    "vitest-browser-react": "^0.0.4"
  }
}
